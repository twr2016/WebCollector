    <script type="text/javascript">
      function showContent1(data_arr) {
        var html= '';
        data = data_arr['data'];
        var nu = 0;
        for(var i in data){
          ++nu;
          var tmptitle = data[i].title;
          html += '<li><a target="_blank" href="'+data[i].url+'">'+data[i].title.gbCut(37)+'</a></li>';
        }
        document.getElementById('blk_shxwxqphb_011').innerHTML = html;
      }
    </script>
    <!-- 感动 end -->

</div>
